import java.util.*;
import java.lang.*;
import java.io.*;


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int inp=sc.nextInt();
		for(int q=0;q<inp;q++)
		{
		    int x=sc.nextInt();
		    if(x<=100)
		    {
		        System.out.println(x);
		    }
		    else
		    {
		        System.out.println(x-10);
		    }
		}
	}
}
